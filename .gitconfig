[core]
	excludesfile = /Users/kevintucker/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[user]
	name = Kevin Tucker
	email = kwtucker4@gmail.com
[alias]
; gs=git status
; gci=git commit -m
; gpu=git push
; gpl=git pull
; ga=git add
; gaa=git add --all
; gdf=git diff
; gdfc=git diff --cached
; gco=git checkout
; gbr=git branch

[url "https://"]
	insteadOf = git://
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
